#include<iostream>
using namespace std;
int n,w[10],m,v[10][10],p[10],x[10];
int max(int a,int b)
{
    return (a>b?a:b);
}
void knapsack()
{
    int i,j;
    for(i=0;i<=n;i++)
    {
       for(j=0;j<=m;j++)
       {
           if(i==0 || j==0)
           v[i][j]=0;
           else if(w[i]>j)
           v[i][j]=v[i-1][j];
           else
           v[i][j]=max(v[i-1][j],v[i-1][j-w[i]]+p[i]);
       }
    }
}
int main()
{
   int i;
   cout<<"\n enter the number of items:";
   cin>>n;
   cout<<"\n enter th weight:";
   for(i=1;i<=n;i++)
   cin>>w[i];
   cout<<"\n enter the values:";
   for(i=1;i<=n;i++)
   cin>>p[i];
   cout<<"\n enter the maximum capacity:";
   cin>>m;
   knapsack();
   cout<<"\n the optional solution is"<<v[n][m]<<"\n";
   return 0;
}

