#include<iostream>
using namespace std;

int main() {
    int a, b, u, v, n, i, j, ne = 1;
    int visited[10] = {0}, min, mincost = 0, cost[10][10];
    cout << "\n enter the num of nodes:";
    cin>>n;
    cout << "\n enter the adjacency matrix";
    for (i = 1; i <= n; i++)
        for (j = 1; j <= n; j++) {
            cin >> cost[i][j];
            if (cost[i][j] == 0)
                cost[i][j] = 999;
        }
    visited[1] = 1;
    cout << "\n";
    while (ne < n) {
        for (i = 1, min = 999; i <= n; i++)
            for (j = 1; j <= n; j++)
                if (cost[i][j] < min)
                    if (visited[i] != 0) {
                        min = cost[i][j];
                        a = u = i;
                        b = v = j;
                    }
        if (visited[u] == 0 || visited[v] == 0) {
            cout << "\n edge:" << ne++ << "%" << a << "," << b << ")" << "cost:" << min;
            mincost += min;
            visited[b] = i;
        }
        cost[a][b] = cost[b][a] = 999;
    }
    cout << "\n mimimum cost" << mincost << "\n";
    return 0;
}

