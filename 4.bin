#include<iostream>

using namespace std;

int a[10][10],n,indegre[10];
void Find_indegre()
{
   int j,i,sum;
   for(j=0;j<=n;j++)
   {
      sum=0;
      for(i=0;i<=n;i++)
      sum+=a[i][j];
      indegre[j]=sum;
   }
}
void topology()
{
   int i,v,u,t[10],s[10],top=-1,k=0;
   Find_indegre();
   for(i=0;i<n;i++)
   {
       if(indegre[i]==0)
       s[++top]=i;
   }
   while(top!=-1)
   {
      u=s[top--];
      t[k++]=u;
      for(v=0;v<n;v++)
      {
	if(a[u][v]==1)
	{
	   indegre[v]--;
	   if(indegre[v]==0)
	   s[++top]=v;
	}
      }
   }
   cout<<"the topological sequence is:\n";
   for(i=0;i<n;i++)
   cout<<" "<<t[i];
}

int main()
{
  int i,j;
  cout<<"enter no of nodes:";
  cin>>n;
  cout<<"\n enter the adjancency matrix:\n";
  for(i=0;i<n;i++)
  {
     for(j=0;j<n;j++)
     cin>>a[i][j];
  }
  topology();
  
  return 0;
}

